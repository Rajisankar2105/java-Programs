import java.util.Scanner;
class Harshad{
public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n=sc.nextInt();
        int temp=n;
        int sum=0;
        while(n>0){
            int a=n%10;
            sum+=a;
            n/=10;
        }
        if(temp%sum==0){
            System.out.println("Harshad");
        }else{
            System.out.println("Not");
        }
}
}
